history:
    {%- for k,v in history.items() %}
    {{ k }}:
        {%- for k, v in v.items() %}
        {%- if not v %}
        {{ k }}: []
        {%- else %}
        {% if v is iterable %}
        {{ k }}:
          {%- for i in v %}
          - {{ i }}
          {% endfor %}
        {% else %}
        {{ k }}: {{ v }}
        {% endif %}
        {% endif %}
        {% endfor %}
    {% endfor %}

